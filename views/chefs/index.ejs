<h2 style='text-align: center'>mesa chefs</h2>

<%chefs.forEach(function(chef) { %>
        <div class='well chefcard'>
        <img class='chefphoto' src=<%= chef.photo%>>
        <h3><a href='/chefs/<%= chef.id %>/show'><%= chef.user.name %></a></h3>
        <h3><%= chef.restaurant %></h3>
        </div>
<% }); %>